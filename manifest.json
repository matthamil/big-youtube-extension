{
    "name": "Big Youtube",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Make Youtube player as big as current window.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["content.js"]
    },
    "permissions": [
        "tabs"
    ]
}